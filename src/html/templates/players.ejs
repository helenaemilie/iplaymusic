<%- include('../partials/before-header.ejs') %>
<body>
   
    <div id="main">
        <div id="image">
            <img src="/assets/img/neffex.jpg"/>
        </div>
        <div id="player">
            <div id="songTitle">Demo</div>
            <div id="buttons">
                <button id="pre" onclick="pre()"><img src="/assets/img/pre.png" height="90%" width="90%"/></button>
                <button id="play" onclick="playOrPauseSong()"><img src="/assets/img/play.png"/></button>
                <button id="next" onclick="next()"><img src="/assets/img/next.png" height="90%" width="90%"/></button>
            </div>
            
            <div id="seek-bar">
                <div id="fill"></div>
                <div id="handle"></div>
            </div>
        </div>
    </div>
</body>
